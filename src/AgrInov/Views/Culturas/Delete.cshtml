@model AgrInov.Models.Cultura

@{
    ViewData["Title"] = "Delete";
}


<h3>Tem certeza de que deseja excluir?</h3>
<div>
    <h4>Cultura</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>
    
    <form asp-action="Delete" style="margin-top:10px; ">
        <input type="hidden" asp-for="Id" />
        <a asp-action="Index" class="btn btn-outline-secondary" style="margin:15px;">Sair</a>
        <input type="submit" value="Confirmar" class="btn btn-outline-dark" style="margin:15px;" />
    </form>
</div>
